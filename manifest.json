{
    "manifest_version": 2,
    "name": "YouTube Add Skipper",
    //"version": "1",
    "permissions": ["tabs" ],
    "content_scripts": [
        {
            "matches": ["https://www.youtube.com/*"],           
            "js": ["content.js"],
            "run_at": "document_end"
        }

    ],
	"background": {
		"scripts": ["background.js"]
	}
}